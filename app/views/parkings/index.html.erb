<table>
	<thead>
		<tr>
			<th>ユーザーid</th>
			<th>パーキングid</th>
			<th>パーキング画像</th>
			<th>カータイプ</th>
			<th>貸出名</th>
			<th>値段（１日の料金）</th>
		</tr>
	</thead>
	<tbody>
		<% @parkings.each do |parking| %>
		<tr>
			<td><%= parking.user.id %></td>
			<td><%= parking.id %></td>
			<td><%= attachment_image_tag parking, :parking_image , :fill, 200, 200%></td>
			<td><%= parking.car_model %></td>
			<td><%= parking.user.name %></td>
			<td><%= parking.price %>円</td>
		</tr>
		<%end%>
	</tbody>
</table>
