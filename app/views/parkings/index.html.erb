<table>
	<thead>
		<tr>
			<th>ユーザーid</th>
			<th>パーキングid</th>
			<th>カータイプ</th>
			<th>貸出名</th>
			<th>住所</th>
			<th>値段（１日の料金）</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		<% @parkings.each do |parking| %>
		<tr>
			<td><%= parking.user.id %></td>
			<td><%= parking.id %></td>
			<td><%= parking.car_model %></td>
			<td><%= parking.user.name %></td>
			<td><%= parking.address%></td>
			<td><%= parking.price %>円</td>
			<td><%= link_to "予約ページ", parking_path(parking) %></td>
		</tr>
		<%end%>
	</tbody>
</table>
