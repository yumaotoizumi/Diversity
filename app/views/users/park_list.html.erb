<table>
	<thead>
		<tr>
			<th>ユーザーid</th>
			<th>パーキングid</th>
			<th>カータイプ</th>
			<th>値段（１日の料金）</th>
			<th>消去</th>
			<th>編集</th>
			<th>画像追加</th>
			<th>画像消去</th>
		</tr>
	</thead>
	<tbody>
		<% @user.parkings.each do |parking| %>
		<tr>
			<td><%= parking.user.id %></td>
			<td><%= parking.id %></td>
			<td><%= parking.car_model %></td>
			<td><%= parking.price %></td>
			<td><%= link_to "削除", parking_path(parking), method: :delete, data: { confirm: '本当に削除しますか?' } %></td>
			<td><%= link_to "編集", edit_parking_path(parking) %></td>
			<td><%= link_to "画像追加", new_parking_parking_image_path(parking) %></td>
			<td><%= link_to "画像消去", parking_parking_images_path(parking) %></td>



		</tr>
		<%end%>
	</tbody>
</table>