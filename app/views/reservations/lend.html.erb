 <h4>駐車場貸出履歴</h4>

 <% @parkings.each do |parkings| %>
 <% parkings.reservations.each do |reservation| %>

<%= reservation.user.name %>
 <%= reservation.parking.price%>円


<%= reservation.start_day.strftime('%y年%m月%d日') %>
<%= reservation.end_day.strftime('%y年%m月%d日') %>
<%  reserve_total = ((reservation.end_day-reservation.start_day) / 86400) %>
 <%= reserve_total.ceil * reservation.parking.price %>円
 <br>
<br>

<% end %>

<% end %>